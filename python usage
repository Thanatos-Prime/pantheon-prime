from pantheon.kernel import Kernel
from pantheon.config import Config

# Initialize the OS with strict governance invariants
os = Kernel(
    governance_level="strict",
    active_daemons=["spider", "hound", "mirror"]
)

# Inject a task into the continuity stream
response = os.process(
    input="Analyze the architectural risks of the new vector database implementation.",
    context_mode="deep_reasoning"
)

print(response.payload)
print(response.daemon_logs)
