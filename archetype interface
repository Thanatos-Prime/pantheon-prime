class Spider(Archetype):
    name = "Spider"
    role = "Structure / Graph Web"

    method propose_operators(ns: NarrativeStructure) -> List[OperatorSpec]:
        if "web" in ns.forces or "network" in ns.forces:
            op = OperatorSpec(name="build_adjacency", formula="A_ij = 1 if linked")
            return [op]
        return []
