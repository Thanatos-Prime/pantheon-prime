# pantheon_memory_engine/models.py
from dataclasses import dataclass
from typing import Optional, List


@dataclass
class Entry:
    id: Optional[int]
    kind: str
    title: str
    body: str
    tags: List[str]


@dataclass
class Link:
    id: Optional[int]
    from_id: int
    to_id: int
    relation: str